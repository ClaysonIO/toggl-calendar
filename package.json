{
  "name": "toggl-calendar",
  "version": "0.1.0",
  "author": "Daniel Clayson",
  "description": "A modified view of the Toggl calendar",
  "private": true,
  "homepage": "./",
  "main": "./public/electron.js",
  "scripts": {
    "start": "vite",
    "build": "vite build"
  },
  "dependencies": {
    "@tippyjs/react": "^4.1.0",
    "axios": "^1.7.2",
    "dayjs": "^1.8.28",
    "electron-is-dev": "^1.2.0",
    "mobx": "^6.12.4",
    "mobx-react": "^9.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^6.24.0",
    "react-router-dom": "^6.24.0",
    "react-select": "^5.8.0",
    "reactjs-popup": "^2.0.3",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/react-dom": "^18.3.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/axios": "^0.14.0",
    "@types/jest": "^24.9.1",
    "@types/node": "^20.14.9",
    "@types/react": "^16.9.35",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "typescript": "^4.7.2",
    "vite": "^4.4.0",
    "vite-plugin-mkcert": "^1.16.0",
    "electron-builder": "^22.7.0",
    "electron": "^9.0.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "io.clayson.toggl",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": "portable"
    },
    "mac": {
      "target": "default"
    }
  }
}
